includes:
    - data/phpstan-baseline.neon
parameters:
    level: 7
    paths:
        - data/wp-content/themes
        - data/wp-content/plugins
    excludePaths:
        - data/vendor/*
        - data/wp-content/plugins/*/vendor/*
    ignoreErrors:
        - '#Call to undefined function add_action\(\)#'
        - '#Call to undefined function add_filter\(\)#'
        - '#Call to undefined function wp_enqueue_script\(\)#'
