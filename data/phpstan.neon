includes:
    - ./phpstan-baseline.neon
parameters:
    level: 7
    paths:
        - ./wp-content/themes
        - ./wp-content/plugins
    excludePaths:
        - ./vendor/*
        - ./wp-content/plugins/*/vendor/*
    ignoreErrors:
        - '#Call to undefined function add_action\(\)#'
        - '#Call to undefined function add_filter\(\)#'
        - '#Call to undefined function wp_enqueue_script\(\)#'
